{
  "name": "ef-brand-campain-huge",
  "version": "1.0.0",
  "description": "ef brand campain huge",
  "main": "index.js",
  "scripts": {
    "lint:msg": "validate-commit-msg",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack",
    "start": "webpack-dev-server --devtool eval --progress --hot --colors --content-base app",
    "deploy": "NODE_ENV=production webpack -p --config webpack.production.config.js",
    "validate": "npm ls",
    "commit": "git cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kumalee/ef-brand-campain-huge.git"
  },
  "keywords": [
    "ef",
    "huge"
  ],
  "author": "Kuma Li",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kumalee/ef-brand-campain-huge/issues"
  },
  "homepage": "https://github.com/kumalee/ef-brand-campain-huge#readme",
  "devDependencies": {
    "commitizen": "^2.8.1",
    "conventional-changelog-cli": "^1.2.0",
    "css-loader": "^0.23.1",
    "cz-conventional-changelog": "^1.1.6",
    "ghooks": "^1.2.1",
    "node-sass": "^3.7.0",
    "open-browser-webpack-plugin": "0.0.2",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "validate-commit-msg": "^2.6.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "config": {
    "ghooks": {
      "commit-msg": "npm run lint:msg"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
